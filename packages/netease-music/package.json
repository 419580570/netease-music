{
  "name": "netease-music",
  "private": true,
  "version": "0.0.0",
  "author": "zc",
  "main": "dist-electron/index.js",
  "description": "electron-vite-vue3 NetestCloudMusic",
  "scripts": {
    "dev": "vite",
    "build": "rimraf release && vue-tsc && vite build && electron-builder",
    "preview": "vite preview"
  },
  "build": {
    "productName": "neteaseCloudMusic",
    "copyright": "Copyright © 2022 zc",
    "directories": {
      "output": "release/${version}"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "electron-main/**/*",
      "electron-preload/**/*"
    ],
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": false
    },
    "releaseInfo": {
      "releaseNotes": "版本更新的具体内容"
    }
  },
  "devDependencies": {
    "@electron/remote": "^2.0.8",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "electron": "17.1.1",
    "electron-builder": "^23.6.0",
    "rimraf": "^3.0.2",
    "unplugin-auto-import": "^0.11.4",
    "vite-plugin-electron": "^0.10.4",
    "vite-plugin-electron-renderer": "^0.11.3"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "pinia": "^2.0.23",
    "vue-router": "^4.1.6"
  }
}
